{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "exclude": ["node_modules"],
  "include": ["src/**/*.ts", "src/**/*.vue", "env.d.ts"],
  "sourceMap": true,
  "allowSyntheticDefaultImports": true,
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
}
